<div class='pull-right' data-ref='right'>
  <a href='<%= o.model.get('photo_file').url %>' data-fancybox-group='group'>
    <img src='<%= o.model.get('photo_file').sm_thumb.url %>' class='media-object' />
  </a>
</div>

<div class='media-body'>
  <div class='date' data-ref='date' data-date-attr='date' data-date-format='MMMM Do, YYYY'></div>

  <div class='venue' data-ref='venue' data-attr='venue'></div>

  <div class='details' data-ref='details' data-attr='details'></div>
</div>
