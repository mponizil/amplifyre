<div class='tour-body'>
  <div class='date' data-attr='date' data-editable='date-input'></div>

  <div class='venue' data-attr='venue' data-editable='text-input'></div>

  <div class='details' data-attr='details' data-editable='text-area'></div>
</div>

<div class='thm'>
  <a href='<%= o.model.get('photo_file').url %>' rel='group' data-fancybox>
    <img src='<%= o.model.get('photo_file').sm_thumb.url %>' />
  </a>
</div>
