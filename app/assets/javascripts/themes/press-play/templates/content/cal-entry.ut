<div class='tour-body'>
  <div class='date' data-html='date' data-editable='date-input'></div>

  <div class='venue' data-html='venue' data-editable='text-input'></div>

  <div class='details' data-html='details' data-editable='text-area'></div>
</div>

<div class='thm'>
  <a href='/assets/images/tour/<%= o.model.get('photo') %>' rel='group' data-fancybox>
    <img src='/assets/images/tour/thms/<%= o.model.get('photo') %>' />
  </a>
</div>
